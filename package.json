{
  "name": "vue-ssr-blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development tsnd -r tsconfig-paths/register src/entry.ts",
    "build": "npm run build:client && npm run build:server && tsc --esModuleInterop --downlevelIteration src/entry.ts --outDir ./dist",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/ssr.ts --outDir dist/server",
    "start": "NODE_ENV=production node ./dist/entry.js",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@aacassandra/vue3-progressbar": "^1.0.3",
    "@popperjs/core": "^2.11.8",
    "@unhead/ssr": "^1.1.26",
    "@unhead/vue": "^1.1.26",
    "axios": "^1.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "highlight.js": "^11.8.0",
    "http-proxy-middleware": "^2.0.6",
    "js-cookie": "^3.0.5",
    "markdown-it": "^13.0.1",
    "pinia": "^2.0.32",
    "swiper": "^9.2.4",
    "v-calendar": "^3.0.3",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/js-cookie": "^3.0.3",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  }
}
